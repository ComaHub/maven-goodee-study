<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sample">
	<insert id="" parameterType="VO">
		INSERT ...
		VALUES #{VO의 getterName}
	</insert>
	
	<insert id="" parameterType="String">
		INSERT ...
		VALUES #{변수명}
	</insert>
	
	<insert id="" parameterType="Map">
		INSERT ...
		VALUES #{Map의 key값}
	</insert>
	
	<insert id="" parameterType="List">
		INSERT ...
		VALUES #{List의 index}
	</insert>
	
	<select id="" resultType="VO">
		<!-- 컬럼명과 VO의 setterName이 일치 -->
	</select>
	
	<select id="" resultType="Map">
		<!-- 컬럼명이 key / 값이 value -->
	</select>
	
	<select id="" resultType="VO">
		<!-- 컬럼명과 VO의 setterName이 일치하지 않는 경우 별칭을 사용 -->
		SELECT board_num AS boardNum FROM NOTICE ...
	</select>
	
	<resultMap type="VO" id="ID">
		<!-- id = PK -->
		<id property="VO의 setterName" column="컬럼명" />
		<!-- result = NOT PK -->
		<result property="VO의 setterName" column="컬럼명" />
		<association property=""></association>
		<collection property=""></collection>
	</resultMap>
	
	<select id="" resultMap="resultMap ID">
	</select>
</mapper>